  <div id="NewClinic" class="modal fade show" role="dialog">
    <div class="modal-dialog" role="content">
      <div class="modal-content">
        <div class="modal-header">
          <h4 class="modal-title">Add a New Clinic</h4>
          <button type="button" class="close" data-dismiss="modal">
            &times;
          </button>
        </div>
        <div class="modal-body">
          <form>
            <div class="form-group row">
              <div class="col-sm-4 order-sm-first offset-sm-1">
                    <label>clinicname</label>  
                  </div>
                  <div class="col-sm-5">
                    

                    <select class="form-control form-control-sm" id="clinicName"  name="clinicName">
                         <option>Blood</option>
                          <option>Back neck and spine</option>
                          <option>Bone joint and muuscle</option>
                          <option>Eye</option>
                          <option>Heart</option>
                          <option>Digestive system</option>
                    </select>

                                <small class="form-text invalid-feedback">this field must be filled
                                         letters</small>
                   </div>
            </div>

            <div class="form-group row">
              <div class="col-sm-4 order-sm-first offset-sm-1">
                    <label>Doctor name</label> 
                  </div>
                  <div class="col-sm-5">
                    <input type="text" name="dname" placeholder="" class="form-control form-control-sm"  id="dname">

                                <small class="form-text invalid-feedback">this field must be filled
                                         letters</small>
                   </div>
            </div>

            <div class="form-group row">
              <div class="col-sm-4 order-sm-first offset-sm-1">
                    <label>Time</label>  
                  </div>
                  <div class="col-sm-5">
                    <input type="text" name="ctime" placeholder="" class="form-control form-control-sm"  id="ctime">

                                <small class="form-text invalid-feedback">this field must be filled
                                         letters</small>
                   </div>
            </div>

            

            

            <div class="form-row">
                            <button type="button" class="btn btn-secondary btn-sm ml-auto" data-dismiss="modal">Cancel</button>
                            <button type="submit" class="btn btn-primary btn-sm ml-1" id = NCAB name="NCAB">Add Clinic</button>        
                        </div>

            

          </form>
          
        </div>
        
      </div>
      
    </div>
    
  </div>


